{
    "repo": "git@github.com:i5heu/Tyche-Test.git",
    "port": 8080,
    "loginAttemptResetSeconds": 300,
    "pullInterval": 30,
    "plugins": [
        {
            "name": "Journal",
            "cron": "5 * * * *",
            "timeout": 300,
            "settings": {
                "journalPath": "Journal"
            }
        },
        {
            "name": "SaveLinkAsPdfArchive",
            "timeout": 300,
            "runOnAllWithType": [
                "md"
            ]
        }
    ]
}